{
    "name": "pettrbalint/cakephp-error-email",
    "description": "CakePHP4.x ErrorEmail Plugin",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "error", "exception", "email", "plugin"],
    "homepage": "https://github.com/pettrbalint/cakephp-error-email/",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Balint",
            "email": "petr.balint@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ebrigham1/cakephp-error-email/issues",
        "source": "https://github.com/ebrigham1/cakephp-error-email/"
    },
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "cakephp/cakephp-codesniffer": "~4.0.0"
    },
    "autoload": {
        "psr-4": {
            "ErrorEmail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ErrorEmail\\Test\\": "tests"
        }
    }
}
